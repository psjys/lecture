// 줄 단위 복사 : ctrl alt up/down
// 줄 단위 이동 : alt up/dowm
// 자동완성 : ctrl space
// 줄 삭제 : ctrl d 
// 실행 : ctrl f11
// 주석 : ctrl /
// 수직 커서 : shift alt a 
// 변수명 동시 수정 : ctrl shift R
// sysout 
// 자동정렬 : ctrl shift f

public class Exer {

	public static void main(String[] args) {
		System.out.println("One");		// println (출력 문자열) : 개행(\n)포함
		System.out.println("Two");		// - "sysout 입력 후 ctrl+Space" 자동완
		
		System.out.print("one");		// print (출력 문자열 ) : 개행 (\n) 미 포
		System.out.print("Two");
		
		System.out.printf("\n%d, %d\n", 1, 2);	// printf ("출력 ㅂ문자열 및 출력 서식", 실인수) :
												// -'%d'는 정수형 서식 문자 
		System.out.printf("%d\n", 123);
		System.out.printf("%5d\n", 123);
		System.out.printf("%.1f", 123.45);
	}

}
