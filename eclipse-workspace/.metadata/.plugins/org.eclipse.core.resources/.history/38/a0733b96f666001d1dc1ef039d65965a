import java.util.Scanner;

// 클래스 : 사용자 정의 타입, 참조
public class Exer {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);  // new 1) 공간할당 2) 해당 주소 반환   // 입력 객체 준비 
			// sc : 변수명
		String name;
		
		System.out.print("이름 : ");
		name = sc.nextLine();
		
		System.out.println(name);
		
		sc.close();
	}
	
}
	








